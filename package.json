{
	"name": "vae-ui",
	"version": "0.0.1",
	"files": [
		"dist/lib/*"
	],
	"main": "dist/lib/vae.js",
	"module": "dist/lib/vae.esm.js",
	"scripts": {
		"website:dev": "vite",
		"build": "vite build",
		"lint": "eslint packages --ext .js,.ts,.vue",
		"lint-fix": "eslint packages --fix --ext .js,.ts,.vue ",
		"format": "prettier --write \"packages/**/*.vue\" \"packages/**/*.ts\" \"website/**/*.vue\" \"website/**/*.ts\"",
		"commit": "git-cz"
	},
	"dependencies": {
		"vue": "^3.0.0-rc.1",
		"vue-router": "^4.0.0-beta.13"
	},
	"devDependencies": {
		"@commitlint/cli": "^11.0.0",
		"@typescript-eslint/eslint-plugin": "^4.7.0",
		"@typescript-eslint/parser": "^4.7.0",
		"@vue/compiler-sfc": "^3.0.0-rc.1",
		"@vue/eslint-config-typescript": "^5.0.2",
		"commitizen": "^4.2.2",
		"commitlint-config-cz": "^0.13.2",
		"cz-customizable": "^6.3.0",
		"eslint": "^7.13.0",
		"eslint-plugin-vue": "^7.0.0-beta.4",
		"husky": "^4.3.0",
		"prettier": "^2.1.1",
		"rollup-plugin-esbuild": "^2.5.0",
		"rollup-plugin-scss": "^2.6.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-vue": "^6.0.0-beta.10",
		"sass": "^1.26.11",
		"typescript": "^4.0.5",
		"vite": "^1.0.0-rc.9",
		"vite-plugin-vuedoc": "^0.0.10",
		"vue-eslint-parser": "^7.1.1"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-customizable"
		}
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{json,css,md}": [
			"npm run format",
			"git add"
		],
		"*.{js,ts,vue}": [
			"npm run lint",
			"npm run format",
			"git add"
		]
	}
}
